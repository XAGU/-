<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DotFlashView">
        <attr name="firstColor" format="color">#fff</attr>
        <attr name="secondColor" format="color">#000</attr>
    </declare-styleable>

    <declare-styleable name="ShadowImageView">
        <attr name="shadowRound" format="dimension" />
        <attr name="shadowSrc" format="reference" />
    </declare-styleable>

    <declare-styleable name="SwipeButton">
        <attr name="inner_text" format="string" />
        <attr name="inner_text_color" format="color" />
        <attr name="inner_text_size" format="dimension" />
        <attr name="inner_text_left_padding" format="dimension" />
        <attr name="inner_text_right_padding" format="dimension" />
        <attr name="inner_text_top_padding" format="dimension" />
        <attr name="inner_text_bottom_padding" format="dimension" />
        <attr name="inner_text_background" format="reference" />
        <attr name="button_image_width" format="dimension" />
        <attr name="button_image_height" format="dimension" />
        <attr name="button_trail_enabled" format="boolean" />
        <attr name="button_trail_drawable" format="reference" />
        <attr name="button_image_disabled" format="reference" />
        <attr name="button_image_enabled" format="reference" />
        <attr name="button_left_padding" format="dimension" />
        <attr name="button_right_padding" format="dimension" />
        <attr name="button_top_padding" format="dimension" />
        <attr name="button_bottom_padding" format="dimension" />
        <attr name="button_background" format="reference" />
        <attr name="has_activate_state" format="boolean" />
        <attr name="initial_state" format="enum">
            <enum name="enabled" value="0" />
            <enum name="disabled" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SlideUnlockView">
        <attr name="slideUnlockBackgroundResource" format="reference" />
        <attr name="slideUnlockBlockResource" format="reference" />
        <attr name="slideEnabledUnlockBlockResource" format="reference" />
        <attr name="slideDisableUnlockBlockResource" format="reference" />
        <attr name="slideEnableText" format="reference" />
        <attr name="slideDisableText" format="reference" />
    </declare-styleable>
    <attr name="minScaleFactor" format="float" />
    <attr name="maxScaleFactor" format="float" />
    <attr name="autoScaleTime" format="integer" />

    <declare-styleable name="ZoomRecyclerView">
        <attr name="minScaleFactor" />
        <attr name="maxScaleFactor" />
        <attr name="autoScaleTime" />
    </declare-styleable>

    <attr name="progressTextSize" format="dimension" />
    <attr name="progressTextColor" format="color" />
    <attr name="progressTextOffset" format="dimension" />
    <attr name="progressNormalColor" format="color" />
    <attr name="progressReachColor" format="color" />
    <attr name="progressNormalSize" format="dimension" />
    <attr name="progressReachSize" format="dimension" />
    <attr name="progressReachWidth" format="dimension" />
    <attr name="progressNormalWidth" format="dimension" />
    <attr name="progressTextVisible" format="boolean" />
    <attr name="progressTextSkewX" format="dimension" />
    <attr name="progressTextPrefix" format="string" />
    <attr name="progressTextSuffix" format="string" />
    <attr name="innerBackgroundColor" format="color" />

    <declare-styleable name="CircleProgressView">
        <attr name="progressNormalColor" />
        <attr name="progressReachColor" />
        <attr name="progressTextColor" />
        <attr name="progressTextSize" />
        <attr name="progressTextOffset" />
        <attr name="progressNormalSize" />
        <attr name="progressReachSize" />
        <attr name="radius" format="dimension" />
        <attr name="progressTextVisible" />
        <attr name="progressStartArc" format="integer" />
        <attr name="progressTextSkewX" />
        <attr name="progressTextPrefix" />
        <attr name="progressTextSuffix" />
        <attr name="innerBackgroundColor" />
        <attr name="progressStyle" format="enum">
            <enum name="Normal" value="0" />
            <enum name="FillInner" value="1" />
            <enum name="FillInnerArc" value="2" />
        </attr>
        <attr name="innerProgressColor" format="color" />
        <attr name="innerPadding" format="dimension" />
        <attr name="outerColor" format="color" />
        <attr name="outerSize" format="dimension" />
        <attr name="reachCapRound" format="boolean" />
    </declare-styleable>
</resources>